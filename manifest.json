{
"manifest_version": 2,
    "name": "Text Replacer",
    "description": "Test plugin for highlighting words and adding on hover event",
    "version": "1.0",
	
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}