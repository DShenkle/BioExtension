{
"manifest_version": 2,
    "name": "Text Replacer",
    "description": "Test plugin for highlighting words and adding on hover event",
    "version": "1.0",
	"browser_action": {
		"default_icon": "off.png",
		"default_title": "icon"
	},
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}